<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title</title>
    <link href="~/Assets/css/normalize.css" rel="stylesheet" />
    <link href="~/Assets/css/style.min.css" rel="stylesheet" />
    <link href="~/Assets/css/font-awesome.css" rel="stylesheet" />
    <link href="~/Assets/css/materialize.min.css" rel="stylesheet" />
    @Scripts.Render("~/bundles/jquery")
    @Styles.Render("~/Content/css")
    <script src="~/Assets/js/materialize.min.js"></script>
</head>
<body class="body-login scrollbar">
    <div class="navbar-fixed">
        <nav>
            <div class="nav-wrapper nav-green">
                <div class="col-md-5">
                    <a href="~/Student/Index" class="brand-logo cursor left"><i class="material-icons">home</i></a>
                </div>
                <div class="col-md-5">
                    @Html.Partial("_LoginPartial")
                </div>
            </div>
        </nav>
    </div>
    @RenderBody()
    <div class="col l12 footer" id="footer">
        <div class="row reset-margin">
            <div class="col l6 s6">
                <i id="website-copyright">@ViewBag.copyright</i>
            </div>
            <div class="col l6 s6 right-align">
                <i id="website-title">@ViewBag.name</i>
            </div>
        </div>
    </div>
